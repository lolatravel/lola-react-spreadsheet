{
  "name": "lola-react-spreadsheet",
  "private": true,
  "version": "1.0.0",
  "repository": "github:lolatravel/lola-react-spreadsheet",
  "type": "module",
  "exports": {
    ".": {
      "module": "./lib/bundle.js",
      "require": "./lib/bundle.cjs",
      "default": "./lib/bundle.js"
    },
    "./dist/": "./dist/"
  },
  "browser": "./lib/bundle.js",
  "main": "./lib/bundle.cjs",
  "module": "./lib/bundle.js",
  "types": "./lib/index.d.ts",
  "files": [
    "dist",
    "lib"
  ],
  "sideEffects": [
    "*.css",
    "*.less"
  ],
  "scripts": {
    "start": "start-storybook --quiet --no-dll -p 6006",
    "build": "rollup --config --no-stdin",
    "postbuild": "node --experimental-modules tools/buildStylesheets.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "eslint": "eslint --ext js,ts,tsx --max-warnings 0 -f codeframe --cache --color src stories test tools",
    "eslint:fix": "npm run eslint -- --fix",
    "typecheck": "tsc -p tsconfig.all.json",
    "build-storybook": "build-storybook --quiet --no-dll",
    "prepublishOnly": "npm install && npm run build && tsc",
    "postpublish": "git push --follow-tags origin HEAD"
  },
  "dependencies": {
    "clsx": "^1.1.1"
  },
  "devDependencies": {
    "@babel/core": "7.14.6",
    "@babel/plugin-transform-runtime": "7.14.5",
    "@babel/preset-env": "7.14.7",
    "@babel/preset-react": "7.14.5",
    "@babel/preset-typescript": "7.14.5",
    "@babel/runtime": "7.14.6",
    "@juggle/resize-observer": "3.3.1",
    "@popperjs/core": "2.9.2",
    "@rollup/plugin-babel": "5.3.0",
    "@rollup/plugin-node-resolve": "13.0.0",
    "@storybook/react": "6.3.1",
    "@testing-library/jest-dom": "5.14.1",
    "@testing-library/react": "12.0.0",
    "@types/faker": "5.5.6",
    "@types/jest": "26.0.23",
    "@types/lodash": "^4.14.165",
    "@types/react": "17.0.11",
    "@types/react-dom": "17.0.8",
    "@types/react-select": "4.0.16",
    "@typescript-eslint/eslint-plugin": "4.28.1",
    "@typescript-eslint/parser": "4.28.1",
    "babel-loader": "^8.2.2",
    "babel-plugin-optimize-clsx": "2.6.2",
    "clean-css": "5.1.3",
    "core-js": "3.15.2",
    "css-loader": "5.2.6",
    "eslint": "7.29.0",
    "eslint-plugin-jest": "24.3.6",
    "eslint-plugin-jest-dom": "3.9.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-react": "7.24.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "faker": "5.5.3",
    "jest": "27.0.6",
    "less": "4.1.1",
    "less-loader": "^7.1.0",
    "lodash": "^4.17.20",
    "mini-css-extract-plugin": "1.6.2",
    "react": "17.0.2",
    "react-dnd": "14.0.2",
    "react-dnd-html5-backend": "14.0.0",
    "react-dom": "17.0.2",
    "react-popper": "2.2.5",
    "react-select": "4.3.1",
    "react-sortable-hoc": "2.0.0",
    "rollup": "2.52.3",
    "typescript": "~4.3.4"
  },
  "peerDependencies": {
    "react": "^16.14 || ^17.0",
    "react-dom": "^16.14 || ^17.0"
  }
}
